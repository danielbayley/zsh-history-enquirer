{
  "name": "zsh-history-enquirer",
  "version": "0.2.8",
  "description": "zsh history search plugin by enquirer",
  "main": "index.js",
  "bin": {
    "fcenquire": "bin/index.js"
  },
  "scripts": {
    "test": "node index.js",
    "preinstall": "command -v zsh || (echo '\\n\\n!! this is a plugin for oh-my-zsh, please install zsh at first \\n\\n' >&2 && exit 1)",
    "postinstall": "./postinstall.zsh",
    "postuninstall": "rm -rf ${ZSH_CUSTOM:-${ZSH}/custom}/plugins/zsh-history-enquirer && perl -i -pe 's/.*zsh-history-enquirer.*//gms' ${HOME}/.zshrc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zthxxx/zsh-history-enquirer.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "keywords": [
    "zsh",
    "history",
    "zsh-history",
    "plugin",
    "plugins",
    "zsh-plugin",
    "zsh-plugins",
    "enquirer",
    "inquirer"
  ],
  "author": "zthxxx",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zthxxx/zsh-history-enquirer/issues"
  },
  "homepage": "https://github.com/zthxxx/zsh-history-enquirer#readme",
  "engines": {
    "node": ">=10.0.0"
  },
  "files": [
    "src",
    "bin",
    "index.js",
    "zsh-history-enquirer.plugin.zsh",
    "postinstall.zsh"
  ],
  "dependencies": {
    "enquirer": "^2.3.2",
    "esm": "^3.2.25",
    "execa": "^2.0.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "husky": "^3.0.5"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}
